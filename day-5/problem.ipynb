   "source": [
    "def cons(a, b):\n",
    "    def pair(f):\n",
    "        return f(a, b)\n",
    "    return pair\n",
    "\n",
    "def car(pair):\n",
    "    return pair(lambda x, y: x)\n",
    "        \n",
    "def cdr(pair):\n",
    "    return pair(lambda x, y: y)\n",
    "        \n",
    "    \n",
    "assert car(cons(3,4)) == 3\n",
    "assert cdr(cons(3,4)) == 4"
   ]
  }
